{
  "manifest_version": 2,

  "name": "Company Tab",
  "description": "Spread information easily",
  "version": "1.01",

  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },

  "browser_action": {
    "default_icon": "favicon.ico"
  },

  "permissions": [
    "tabs",
    "identity"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"



}
